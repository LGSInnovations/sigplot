{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["js", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|doc)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./doc",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/minami"
  }
}
